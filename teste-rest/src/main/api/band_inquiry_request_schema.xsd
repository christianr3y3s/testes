<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://www.example.org/band_inquiry_request_schema" 
	xmlns:tns="http://www.example.org/band_inquiry_request_schema" 
	elementFormDefault="qualified">

    <element name="bandInquiryRequest" type="tns:requestType"></element>
    

    <complexType name="requestType">
    	<sequence>
    		<element name="band" type="tns:bandType" maxOccurs="unbounded" minOccurs="1"></element>
    	</sequence>
    </complexType>

    <complexType name="bandType">
    	<sequence>
    		<element name="bandId" type="string"></element>
    	</sequence>
    </complexType>
</schema>