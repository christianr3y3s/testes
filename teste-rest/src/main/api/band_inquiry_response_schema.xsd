<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://www.example.org/band_inquiry_response_schema" 
	xmlns:tns="http://www.example.org/band_inquiry_response_schema" elementFormDefault="qualified">

    <element name="bandInquiryResponse" type="tns:responseType"></element>
    
    <complexType name="responseType">
    	<sequence>
    		<element name="band" type="tns:bandType" maxOccurs="unbounded" minOccurs="1"></element>
    	</sequence>
    </complexType>

    <complexType name="bandType">
    	<sequence>
    		<element name="bandId" type="string"></element>
    		<element name="nome" type="string"></element>
    		<element name="anoFormacao" type="string"></element>
    	</sequence>
    </complexType>
</schema>